{
  "sourceType": "unambiguous",
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "entry",
        "corejs": "3"
      }
    ]
  ],

  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "corejs": "3",
        "helpers": true,
        "regenerator": true,
        "useESModules": true
      }
    ]
  ]
}